echo "enter the num1"
read num1
echo "enter the num2"
read num2
echo "enter the num3"
read num3
if [ $num1 -gt $num2 ] && [ $num1 -gt $num3 ]
then
        echo "num1 is bigger'$num1'"
elif [ $num2 -gt $num3 ] && [ $num2 -gt $num1 ]
then
        echo "num2 is bigger'$num2'"
else
        echo "num3 is greater"
fi
